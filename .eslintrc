{
  parser: "babel-eslint",
	extends: ["eslint:recommended", "plugin:react/recommended"],
	env: {
		browser: true,
	},
	rules: {
		"no-unused-vars": [0, { "varsIgnorePattern": "^h$" }],
		"no-mixed-spaces-and-tabs": [0, { "varsIgnorePattern": "^h$" }],
		"react/prop-types": [0, { "varsIgnorePattern": "^h$" }]
	}
}